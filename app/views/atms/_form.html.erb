<%= form_for @bank.atms.build do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :address %><br>
    <%= form.text_field :address %>
    <%= form.hidden_field :bank_id, :value => @bank.id %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>