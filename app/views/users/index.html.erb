<h1>Listing Users</h1>
<%= link_to 'New user', new_user_path %>
<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>father_name</th>
    <th>mother_name</th>
    <th>age</th>
    <th>Address</th>
    <th>contact_no</th>
    <th colspan="3"></th>
  </tr>
  <% @branches.each do |branch| %>
    <tr>
      <td><%= branch.id %></td>
      <td><%= branch.name %></td>
      <td><%= branch.father_name %></td>
      <td><%= branch.mother_name %></td>
      <td><%= branch.age %></td>
      <td><%= branch.address %></td>
      <td><%= branch.contact_no %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>