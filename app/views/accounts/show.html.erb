<!DOCTYPE html>
<html>
  <head>
    <meta charset=“UTF-8”>
    <meta lang=“english”>
    <meta title=“banking-system”>
    <title>Banking System</title>
  </head>
  <body>
    <div id="account">
      <label class="accountno header"></label><br />
      <lable class="accounttype header"></lable><br />
    </div>
    
    <div id="transactions">
     
      <h1>Transactions</h1>
      <table class="list">
        <tr>
          <th>Transaction Type</th>
          <th>Amount</th>
          <th colspan="3"></th>
        </tr>
      </table>
    </div>
    <button class="button new" id="newtransaction">Add New Transaction</button>
    <div id="addtransaction">
      <h1>Add transaction</h1>
      <form class="newtransaction">
        <input class="accountId" type="text" name="AccountId" hidden="true">
        <label class="heading"> Transaction Type:</label>
        <select class="data" id="TransactionType"type="text" name="TransactionType">
        	<option>deposit</option>
        	<option>withdraw</option>
        </select>
        <label class="heading">Amount:</label>
        <input class="data" id="Amount" type="text" name="Amount">
        <br>
        <label class="heading">Select-Atm:</label>
        <select class="data" id="Atm" name="Atm"></select>
        <input class="button submit" type="button" value="Submit">
      </form> 
    </div> 
    <hr>
    
  </body>
  <script type="text/javascript">
    jQuery(document).ready(function(){
      var pageLoaded = new ShowAccount.pageLoaded();
    });
  </script>
</html>